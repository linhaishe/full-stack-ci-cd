{
  "name": "fullstack-ci-cd-repo",
  "version": "1.0.0",
  "description": "A boilerplate",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "nodemon --exec tsx server/index.ts",
    "dev:client": "cd client && vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "test:dev": "npx tsx index.ts",
    "build": "npm install && cd client && npm install && vite build"
  },
  "engines": {
    "node": ">=20"
  },
  "workspaces": [
    "client",
    "server"
  ],
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "axios": "^0.19.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chokidar": "^3.3.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "module-alias": "^2.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^2.0.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.8.1",
    "vite": "^7.1.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.1",
    "webpack-hot-middleware": "^2.25.0"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "client/*",
      "server/*"
    ]
  },
  "_moduleAliases": {
    "@util": "server/util",
    "@controllers": "server/controllers",
    "@middleware": "server/middleware",
    "@root": ".",
    "Utilities": "client/util",
    "Components": "client/components",
    "Assets": "client/assets"
  }
}
